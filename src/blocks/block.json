{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rrze/rrze-video",
	"version": "0.1.0",
	"title": "RRZE Video",
	"category": "widgets",
	"icon": "smiley",
	"description": "Embeds videos from FAUVideo, YouTube & Co.",
	"supports": {
		"html": false
	},
	"textdomain": "rrze-video",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"id": {
			"type": "number",
			"default": null
		},
		"url": {
			"type": "string",
			"default": "https://www.fau.tv/webplayer/id/95867"
		},
		"class": {
			"type": "string",
			"default": null
		},
		"titletag": {
			"type": "string",
			"default": null
		},
		"poster": {
			"type": "string",
			"default": null
		},
		"rand": {
			"type": "boolean",
			"default": null
		}
	}
}
